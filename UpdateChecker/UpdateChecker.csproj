<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <TargetFrameworks>net5.0;netcoreapp3.1;netstandard2.0</TargetFrameworks>
      <LangVersion>latest</LangVersion>
      <Nullable>enable</Nullable>
      <Authors>HMBSbige</Authors>
      <Description>Check for .NET app updates.</Description>
      <RepositoryUrl>https://github.com/HMBSbige/UpdateChecker</RepositoryUrl>
      <PackageProjectUrl>https://github.com/HMBSbige/UpdateChecker</PackageProjectUrl>
      <PackageTags>UpdateChecker</PackageTags>
      <Version>5.0.0</Version>
      <Copyright>Copyright Â© HMBSbige 2020</Copyright>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <PackageId>HMBSbige.UpdateChecker</PackageId>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
